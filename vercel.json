{
  "version": 2,
  "builds": [
    {
      "src": "selector.html",
      "use": "@vercel/static"
    },
    {
      "src": "docs/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "HSX_SITE_EXAMPLE/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    
    {
      "source": "/",
      "destination": "/selector.html"
    },
    
    
    {
      "source": "/animation",
      "destination": "/docs/index.html"
    },
    {
      "source": "/animation/:path*",
      "destination": "/docs/:path*"
    },
    
    
    {
      "source": "/example",
      "destination": "/HSX_SITE_EXAMPLE/index.html"
    },
    {
      "source": "/example/:path*",
      "destination": "/HSX_SITE_EXAMPLE/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/animation",
      "permanent": true
    },
    {
      "source": "/HSX_SITE_EXAMPLE",
      "destination": "/example", 
      "permanent": true
    }
  ]
}
