{
  "version": 2,
  "builds": [
    {
      "src": "docs/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "HSX_SITE_EXAMPLE/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
   
    {
      "source": "/",
      "destination": "/docs/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/docs/$1"
    },
    
    
    {
      "source": "/example",
      "destination": "/HSX_SITE_EXAMPLE/index.html"
    },
    {
      "source": "/example/:path*",
      "destination": "/HSX_SITE_EXAMPLE/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/hsx-site-example",
      "destination": "/example",
      "permanent": false
    }
  ]
}
